<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
			fx:controller="net.erenalyoruk.hms.controller.ProfileController">
	<HBox spacing="20.0">
		<padding>
			<Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
		</padding>
		<fx:include maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" source="navigation.fxml"/>
		<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="20.0">
			<Text strokeType="OUTSIDE" strokeWidth="0.0" text="Profile">
				<font>
					<Font name="System Bold" size="24.0"/>
				</font>
			</Text>
			<GridPane hgap="10.0" vgap="5.0">
				<columnConstraints>
					<ColumnConstraints/>
					<ColumnConstraints hgrow="SOMETIMES"/>
				</columnConstraints>
				<rowConstraints>
					<RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES"/>
					<RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES"/>
					<RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES"/>
					<RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES"/>
					<RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES"/>
					<RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES"/>
					<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
				</rowConstraints>
				<Text strokeType="OUTSIDE" strokeWidth="0.0" text="Citizen Number:">
					<font>
						<Font name="System Bold" size="12.0"/>
					</font>
				</Text>
				<Text fx:id="citizenNumberText" strokeType="OUTSIDE" strokeWidth="0.0" text="12345678910"
					  GridPane.columnIndex="1"/>
				<Text strokeType="OUTSIDE" strokeWidth="0.0" text="First Name:" GridPane.rowIndex="1">
					<font>
						<Font name="System Bold" size="12.0"/>
					</font>
				</Text>
				<Text fx:id="firstNameText" strokeType="OUTSIDE" strokeWidth="0.0" text="John"
					  GridPane.columnIndex="1" GridPane.rowIndex="1"/>
				<Text strokeType="OUTSIDE" strokeWidth="0.0" text="Last Name:" GridPane.rowIndex="2">
					<font>
						<Font name="System Bold" size="12.0"/>
					</font>
				</Text>
				<Text fx:id="lastNameText" strokeType="OUTSIDE" strokeWidth="0.0" text="Doe"
					  GridPane.columnIndex="1" GridPane.rowIndex="2"/>
				<Text strokeType="OUTSIDE" strokeWidth="0.0" text="Email:" GridPane.rowIndex="3">
					<font>
						<Font name="System Bold" size="12.0"/>
					</font>
				</Text>
				<Text fx:id="emailText" strokeType="OUTSIDE" strokeWidth="0.0" text="test@test.com"
					  GridPane.columnIndex="1" GridPane.rowIndex="3"/>
				<Text strokeType="OUTSIDE" strokeWidth="0.0" text="Gender:" GridPane.rowIndex="4">
					<font>
						<Font name="System Bold" size="12.0"/>
					</font>
				</Text>
				<Text fx:id="genderText" strokeType="OUTSIDE" strokeWidth="0.0" text="MALE"
					  GridPane.columnIndex="1" GridPane.rowIndex="4"/>
				<Text strokeType="OUTSIDE" strokeWidth="0.0" text="Birth Date:" GridPane.rowIndex="5">
					<font>
						<Font name="System Bold" size="12.0"/>
					</font>
				</Text>
				<Text fx:id="birthDateText" strokeType="OUTSIDE" strokeWidth="0.0" text="27.07.2002"
					  GridPane.columnIndex="1" GridPane.rowIndex="5"/>
			</GridPane>
		</VBox>
	</HBox>
</AnchorPane>
